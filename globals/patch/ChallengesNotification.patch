% This file handle the notification who appear when you complete a challenge
% Also have the code for a few challenges, although most challenges are in their respective scripts (ej. look for Rowf script for his challenges)

#new:Data $Challenge_PopUp_Data
{
00000000
% XX000000 % Notification Active Flag
% 00XX0000 % Star Points Gained
% 0000XXXX % Challenge String ID
}

#new:Data $Challenge_PopUp_Data2
{
00000000
% XX000000 % Notification Timer
% 00XX0000 % Opacity
% 0000XX00 % Notification State
% 000000XX % unused
}

% Needed by BadgeBelts.patch, when you pause the menu to see the cheev unlocked
#export $Challenge_PopUp_Data
#export $Challenge_PopUp_Data2

#new:Function $Function_ChallengeNotification
{
PUSH    RA, S0, S1, S2, S3, V0, V1, A1, A2, A3
LIO     T0, $Challenge_PopUp_Data
LBU     T1, 0 (T0) % Load Notif Flag
BEQ     T1, R0, .End
NOP
% A notification need to be displayed
LIO     T0, $Challenge_PopUp_Data2
LBU     S0, 0 (T0) % Load PopUp timer
LBU     S1, 1 (T0) % Opacity
LBU     S2, 2 (T0) % PopUp State ID
LIO     T1, $Challenge_PopUp_Data
LBU     S3, 1 (T1) % Star Points earned
BNE     S2, R0, .CheckState1
NOP
% Start Notification Animation
ADDIU   T1, R0, 1
BEQ     R0, R0, .End
SB      T1, 2 (T0) % Set state as: Start Animation
.CheckState1
ADDIU   T1, R0, 1
BNE     S2, T1, .CheckState2
NOP
ADDIU   S1, S1, 20 % How fast the notification appears
% Check if the box is fully visible
SLTIU   T1, S1, 100
BNE     T1, R0, .SaveNewOpacity
NOP
% The box is fully displayed
ADDIU   S1, R0, FF % Max
% Set Box State to 2 (Start Animation Ended)
ADDIU   T1, R0, 2
SB      T1, 2 (T0)
.SaveNewOpacity
BEQ     R0, R0, .PrintNotifBox
SB      S1, 1 (T0) % Save new Opacity
.CheckState2
ADDIU   T1, R0, 2
BNE     S2, T1, .CheckState3
NOP
% Give Star Points
BEQ     S3, R0, .BypassGivingSP
NOP
ADDIU   T0, R0, 1
LAHU    T1, 8035612E
ADDIU   T1, T1, 1 % Add 1 SP
SAH     T1, 8035612E
SUBU    S3, S3, T0 % Sub 1 SP from the ones remaining to give
LIO     T0, $Challenge_PopUp_Data
SB      S3, 1 (T0)
% Play Sound
ADDIU	A0, R0, 0215 % Star Point
CLEAR 	A1
COPY 	A2, A1
JAL 	~Func:_play_sound
COPY 	A3, A1
.BypassGivingSP
LABU    T0, 80074094 % Game State / 00 = Overworld
BNE     T0, R0, .BypassStartButton
NOP
.CheckStartInput
% Track Start Button
LABU    T0, 80074036 % Inputs % bit4 = Start
SLL    	T0, T0, 27`
SRL 	T0, T0, 31` % Move all bits to just leave bit4
BEQ	    T0, R0, .BypassStartButton % If the Start button wasn't pressed
NOP
% Pause the game and open the challenge unlocked
% ADDIU 	A2, R0, 1
% LIO 	A1, *GoToExtraTab
% JAL 	~Func:set_variable
% RESERVED
%% The code that open the extra tab and open the challenge unlocked is at BadgeBelts.patch
% In Hook 422C
.BypassStartButton
LIO     T0, $Challenge_PopUp_Data2
ADDIU   S0, S0, 4 % How long the notification will stay
SB      S0, 0 (T0) % Save Timer
% The timer has ended?
SLTIU   T1, S0, 100
BNE     T1, R0, .PrintNotifBox
NOP
% Has ended, set box state to 3 (Dissapear)
% Set timer back to 0
SB      R0, 0 (T0) % Save Timer
ADDIU   T1, R0, 3 % State 3
SB      T1, 2 (T0)
BEQ     R0, R0, .PrintNotifBox
NOP
.CheckState3
ADDIU   T2, R0, 20 % How fast the notification dissappear
SUBU    S1, S1, T2
% Check if the box is fully invisible
SLTI    T1, S1, 0
BEQ     T1, R0, .SaveNewOpacity
NOP
% The box ending fade animation has ended
% Restart all Animation Data
SB      R0, 0 (T0) % Load PopUp timer
SB      R0, 1 (T0) % Opacity
SB      R0, 2 (T0) % PopUp State ID
SAB     R0, $Challenge_PopUp_Data
BEQ     R0, R0, .End
NOP
.PrintNotifBox
JAL     $Function_NotificationBox
NOP
.End
POP     RA, S0, S1, S2, S3, V0, V1, A1, A2, A3
JR      RA
ADDIU   V0, R0, 2
}

#new:Function $Function_NotificationBox
{
PUSH	RA, A0, A1, A2, A3, T0, T1, T2
% Draw Box Frame
ADDIU	A0, R0, 12  % posX
ADDIU	A1, R0, 3B  % posY
ADDIU	A2, R0, 81 % sizeX
ADDIU	A3, R0, 2D % sizeY
ADDIU   T2, R0, FF % fogRGB
COPY    T1, S1  % Load Opacity
% Check if the opacity is above 60
SLTIU   T0, T1, 60
BEQL    T0, R0, .LockOpacity
ADDIU	T1, R0, 60 % opacity
.LockOpacity
JAL		$DrawBox_Notification
ADDIU	T0, R0, 0  % style
% Draw Box BG
ADDIU	A0, R0, 10  % posX
ADDIU	A1, R0, 39  % posY
ADDIU	A2, R0, 85 % sizeX
ADDIU	A3, R0, 30 % sizeY
ADDIU   T2, R0, 00 % fogRGB
COPY    T1, S1  % Load Opacity
JAL		$DrawBox_Notification
ADDIU	T0, R0, 1  % style
% Draw Strings
% START (Hold)
% Check if the player is in the overworld
LABU    T0, 80074094 % Game State / 00 = Overworld
BNE     T0, R0, .DontPrintStart
NOP
% Check if the player can currently move Mario
LABU    T0, 802BFF0B % Lakitester's cloud state
BEQ     T0, R0, .CheckInputsState % the player isn't using Lakilester
NOP
.CheckLakisInputs
% Lakilester use his own Inputs Disabled Byte Flag
LABU    T0, 8010EBC4 % Lakilester's DisablePlayerInput Byte Flag
BNE	    T0, R0, .DontPrintStart % Inputs currently locked
NOP
% The player is mounting laki but the inputs are currently unlocked
BEQ     R0, R0, .PrintStart
NOP
% Check if the inputs are locked
.CheckInputsState
LABU    T0, 8010EFCA % Player Flags % bit5=Locked Inputs
SLL    	T0, T0, 26`
SRL 	T0, T0, 31` % Move all bits to just leave bit5
BNE	    T0, R0, .DontPrintStart % Inputs currently locked
NOP
.PrintStart
% Draw Box
ADDIU	A0, R0, F5 % posX
ADDIU	A1, R0, C6 % posY
LABU    A2, $SkipCs_Data % ?? = MAX % sizeX
ADDIU	A3, R0, 14 % sizeY
ADDIU	T0, R0, 3  % style
JAL		$DrawBox
ADDIU	T1, R0, FF % opacity
% Draw START string
LIO     A0, $String_START
ORI     A1, R0, 003D % x pos
COPY    A3, S1 % Load Opacity
SW      R0, 0014 (SP) % text fx / 0 = none
JAL     ~Func:draw_string
ORI     A2, R0, 0069 % y pos
% Challenge
.DontPrintStart
LIO     A0, $String_Challenge
ORI     A1, R0, 001C % x pos
COPY    A3, S1 % Load Opacity
JAL     ~Func:draw_string
ORI     A2, R0, 003E % y pos
% Challenge Title
LIO     T0, $Challenge_PopUp_Data
LHU     T0, 2 (T0)
LUI     A0, 002F % String Bank ID
ADDU    A0, A0, T0 % load Challenge Text ID
ORI     A1, R0, 0019 % x pos
COPY    A3, S1 % Load Opacity
JAL     ~Func:draw_string
ORI     A2, R0, 0050 % y pos
POP		RA, A0, A1, A2, A3, T0, T1, T2
JR		RA
NOP
}

